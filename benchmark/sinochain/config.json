{
    "blockchain": {
      "type": "sinochain",
      "config": "benchmark/sinochain/sinochain.json"
    },
    "test": {
      "name": "sinochain test",
      "description": "Sinochain test!",
      "clients": {
        "type": "zookeeper",
        "unfinished": 50000,
        "zoo" : {
          "server": "127.0.0.1:12181",
          "clientsPerHost": 1
        }
      },
      "rounds": [
        {
          "label": "open-account",
          "txNumber": [
            100000
          ],
          "rateControl": [
            {
              "type": "fixed-rate",
              "opts": {
                "tps": 10000
              }
            }
          ],
          "callback": "benchmark/sinochain/open.js"
        }
      ]
    },
    "monitor": {
      "type": [
        
      ],
      "process": [
        {
          "command": "node",
          "arguments": "main.js",
          "multiOutput": "avg"
        }
      ],
      "interval": 1
    }
  }